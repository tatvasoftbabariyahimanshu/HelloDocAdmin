<div class="row justify-content-between mb-2 ">
    <div class="text-start col-md-4 ">
        <div class=" row">

            <div class="col-md-6">

                <div class="inputs-control mt-1 w-100 p-0 ">
                    <input placeholder=" " class="input-outline input-outline-search " id="myInput" onkeyup="myFunction()" type="text">
                    <label class="inputs-lable">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                            <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0" />
                        </svg> Search Patient
                    </label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="dropdown w-100 mt-1   ">
                    <div class="inputs-control w-100 p-0 ">
                        <input class="  input-outline input-outline-search h-100 " type="button" data-bs-toggle="dropdown" aria-expanded="false" onfocus="false" />


                        <label class="inputs-lable-ser ">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                                <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0" />
                            </svg> All Region
                        </label>



                        <ul class="dropdown-menu">
                            <li><a class="dropdown-item" href="#">Action</a></li>
                            <li><a class="dropdown-item" href="#">Another action</a></li>
                            <li><a class="dropdown-item" href="#">Something else here</a></li>
                        </ul>
                    </div>

                </div>
            </div>
        </div>



    </div>

    <div class="text-start mt-2 col-md-4">
        <div class="d-flex  text-start  justify-content-end ">
          @*  <button class="btn  btn-outline-info btn-sm mx-2 cir-text">all </button>*@
            <div type="button" class="d-flex align-items-center " onclick="filterSelection('all')">
                <div type="button" class="btn btn-outline-info ">All</div>
            </div>
            <div type="button" class="d-flex align-items-center " onclick="filterSelection('bg-patient-row')">
                <div class="cir-patient mx-2">
                </div>
                <span class="cir-text">
                    Patient
                </span>
            </div>
            <div type="button" class="d-flex align-items-center " onclick="filterSelection('bg-family-row')">
                <div class="cir-family mx-2">
                </div>
                <span class="cir-text" >
                    Family/Friend
                </span>
            </div>
            <div type="button"  class="d-flex align-items-center " onclick="filterSelection('bg-business-row')">
                <div class="cir-Business mx-2">
                </div>
                <span class="cir-text" >
                    Business
                </span>
            </div>
            <div type="button"  class="d-flex align-items-center " onclick="filterSelection('bg-concierge-row')">
                <div class="cir-Concierge mx-2">
                </div>
                <span class="cir-text" >
                    Concierge
                </span>
            </div>
        </div>
    </div>


</div>