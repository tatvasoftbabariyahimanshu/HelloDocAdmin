<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HELLO_DOC</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css">
    <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HELLO_DOC.styles.css" asp-append-version="true" />
    <link href='https://fonts.googleapis.com/css?family=Poppins' rel='stylesheet'>
    <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    <script src="https://code.jquery.com/jquery-3.7.1.min.js"
            integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
</head>
<body data-bs-theme="" class="d-flex flex-column align-items-center justify-content-between text-center">
    <div>
        <button class=" btn-outline-info btn btn-sm position-fixed " onclick="mode()" style="right:10px;top:10px">
            <span id="thm"> <i class="fa-solid fa-sun"></i></span>
            <span id="thm2"> <i class="fa-solid fa-moon"></i></span>
        </button>
    </div>
    <div class="card container card-login-forgot border-0 bg-transparent d-flex flex-column align-items-center justify-content-between ">
        <div class="bg-image"></div>
        <div class="">
            <img src="~/images/Fig56. Patient site 1.png" class="img-fluid rounded-start mt-3 py-4 logo" alt="...">
        </div>
        <div>
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
        <div></div>
    </div>
    <div>
        <span class="d-flex flex-row justify-content-between">
            <a href="#" class="px-2 footer-text-login">Tearms and condition</a><p>|</p><a href="#" class="px-2 footer-text-login">privicy policy</a>
        </span>
    </div>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/custom.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
